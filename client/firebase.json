{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "!/api/analytics/**",
        "destination": "/index.html"
      },{
        "source": "/api/analytics/**",
        "function": "redirectHeroku"
      }
    ]
  }
}
